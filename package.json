    "description": "My github page",
    "scripts": {
        "prepare": "husky install",
        "lint": "eslint ./",
        "lint-fix": "eslint --fix ./",
        "format": "prettier --write .",
        "watch": "tailwindcss -i ./css/app.css -o ./dist/output.css --watch",
